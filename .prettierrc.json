{
  "endOfLine": "crlf",
  "trailingComma": "all",
  "printWidth": 175,
  "semi": true,
  "singleQuote": true,
  "tabWidth": 2,
  "singleAttributePerLine": true,
  "importOrder": [
    "^next/(.*)$",
    "^react/(.*)$",
    "^react",
    "<THIRD_PARTY_MODULES>",
    "[./]*.module.*",
    "^components/(.*)$",
    "/store/(.*)$",
    "/http/(.*)$",
    "/hooks/(.*)$",
    "/shared/(.*)$",
    "^public/(.*)$",
    "^styles/(.*)$"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
